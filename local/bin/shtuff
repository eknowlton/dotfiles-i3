#!/home/ethan/.asdf/installs/python/3.7.3/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'shtuff==0.3.1','console_scripts','shtuff'
__requires__ = 'shtuff==0.3.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('shtuff==0.3.1', 'console_scripts', 'shtuff')()
    )
